<script>
        var w = window.innerWidth*.56;
        $("#abs").css('left', ((w / 2) - 400));
        
        var blade = $("#thing").width();
        $("#menu >li").width((blade / 5) - 2);

        $("#newsMenu >li").width((blade / 5) - 2);
        
        //$('.nav').tab('show');
        
        $('ul.nav-tabs>li').click(function (e) {
          $('ul.nav-tabs>li').removeClass('active');
          $(this).addClass('active');
        });
        
        $('#xer').click(function (e) {
              $("#leftWrapper").html("");
              $("#leftWrapper").css("visibility", "hidden");
        });

  </script>
